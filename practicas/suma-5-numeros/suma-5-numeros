using System;

class Program
{
    static void Main()
    {
        int suma5 = 0;
        for (int i = 1; i <= 5; i++)
        {
            Console.Write($"Ingrese nÃºmero {i}: ");
            suma5 += int.Parse(Console.ReadLine());
        }
        Console.WriteLine("Suma total = " + suma5);
    }
}
