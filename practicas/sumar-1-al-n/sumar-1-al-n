using System;

class Program
{
    static void Main()
    {
        Console.Write("Ingrese un n√∫mero N: ");
        int N = int.Parse(Console.ReadLine());
        int suma = 0, contador = 1;

        while (contador <= N)
        {
            suma += contador;
            contador++;
        }

        Console.WriteLine("Suma = " + suma);
    }
}
